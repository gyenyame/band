<h1>Listing venues</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zipcode</th>
    <th>Website</th>
    <th>Phone</th>
  </tr>

<% for venue in @venues %>
  <tr>
    <td><%=h venue.name %></td>
    <td><%=h venue.description %></td>
    <td><%=h venue.address %></td>
    <td><%=h venue.city %></td>
    <td><%=h venue.state %></td>
    <td><%=h venue.zipcode %></td>
    <td><%=h venue.website %></td>
    <td><%=h venue.phone %></td>
    <td><%= link_to 'Show', venue %></td>
    <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
    <td><%= link_to 'Destroy', venue, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New venue', new_venue_path %>
