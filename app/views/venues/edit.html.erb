<h1>Editing venue</h1>

<%= error_messages_for :venue %>

<% form_for(@venue) do |f| %>
  <p>
		<%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
		<%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
		<%= f.label :address %><br />
    <%= f.text_field :address %>
  </p>
  <p>
		<%= f.label :city %><br />
    <%= f.text_field :city %>
  </p>
  <p>
		<%= f.label :state %><br />
    <%= f.text_field :state %>
  </p>
  <p>
		<%= f.label :zipcode %><br />
    <%= f.text_field :zipcode %>
  </p>
  <p>
		<%= f.label :website %><br />
    <%= f.text_field :website %>
  </p>
  <p>
		<%= f.label :phone %><br />
    <%= f.text_field :phone %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @venue %> |
<%= link_to 'Back', venues_path %>
